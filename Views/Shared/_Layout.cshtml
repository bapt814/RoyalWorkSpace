<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mon application ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


    <meta charset="utf-8">

    <link rel="apple-touch-icon" sizes="180x180" href="~/Scripts/assets/ico/apple-touch-icon.png">

    <link rel="icon" type="image/png" sizes="32x32" href="~/Scripts/assets/ico/favicon-32x32.png">

    <link rel="icon" type="image/png" sizes="16x16" href="~/Scripts/assets/ico/favicon-16x16.png">

    <link rel="manifest" href="~/Scripts/assets/ico/manifest.json">

    <link rel="mask-icon" href="~/Scripts/assets/ico/safari-pinned-tab.svg" color="#5bbad5">

    <link rel="shortcut icon" href="~/Scripts/assets/ico/favicon.ico">

    <meta name="msapplication-config" content="~/Scripts/assets/ico/browserconfig.xml">

    <meta name="theme-color" content="#ffffff">


    <title>La Casa Royal</title>


    <!-- CSS Plugins -->

    <link rel="stylesheet" type="text/css" href="~/Scripts/assets/plugins/flickity/flickity.min.css">


    <!-- CSS Global -->

    <link rel="stylesheet" type="text/css" href="~/Scripts/assets/css/theme.min.css">


</head>

<body data-spy="scroll" data-offset="71" data-target=".navbar:visible">


    <!-- NAVBAR

    ================================================== -->

    <nav class="navbar navbar-light navbar-expand-lg fixed-top">

        <div class="container">


            <!-- Navbar: Brand -->

            <a class="navbar-brand d-lg-none" href="index.html">Casa Royal</a>

            <!-- Navbar: Toggler -->

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">

                <span class="navbar-toggler-icon"></span>

            </button>



            <!-- Navbar: Collapse -->

            <div class="collapse navbar-collapse" id="navbarSupportedContent">


                <!-- Navbar navigation: Left -->

                <ul class="navbar-nav mr-auto">

                    <li class="nav-item">

                        <a class="nav-link" href="about-us.html">A propos de nous</a>

                    </li>

                    <li class="nav-item dropdown">

                        <a class="nav-link" href="menu.html">Menu</a>

                    </li>

                    <li class="nav-item">

                        <a class="nav-link" href="reservation.html">Reservation</a>

                    </li>

                </ul>


                <!-- Navbar: Brand -->

                <a class="navbar-brand d-none d-lg-flex" href="index.html">

                    Casa Royal's

                </a>


                <!-- Navbar navigation: Right -->

                <ul class="navbar-nav ml-auto">

                    <li class="nav-item">

                        <a class="nav-link" href="news-and-events.html">After Works & Evenements</a>

                    </li>

                    <li class="nav-item">

                        <a class="nav-link" href="gallery.html">Gallerie</a>

                    </li>

                    <li class="nav-item">

                        <a class="nav-link" href="contact-us.html">Contact</a>

                    </li>

                </ul>


            </div> <!-- / .navbar-collapse -->


        </div> <!-- / .container -->

    </nav> <!-- / .navbar -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/assets")
    @RenderSection("scripts", required: false)

    @RenderBody()


</body>
</html>
